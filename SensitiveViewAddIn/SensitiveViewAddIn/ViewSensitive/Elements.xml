<?xml version="1.0" encoding="utf-8"?>
<Elements xmlns="http://schemas.microsoft.com/sharepoint/">
  <ClientWebPart Name="NDASensitiveView" Title="NDASensitiveView" Description="SensitiveView" DefaultWidth="1500" DefaultHeight="600">

    <!-- Content element identifies the location of the page that will render inside the client web part
         Properties are referenced on the query string using the pattern _propertyName_
         Example: Src="~appWebUrl/Pages/ClientWebPart1.aspx?Property1=_property1_" -->
    <Content Type="html" Src="~remoteAppUrl/Pages/ViewSensitive.aspx?{StandardTokens}&amp;SensitiveViewLists=_SensitiveViewLists_" />

    <!-- Define properties in the Properties element.
         Remember to put Property Name on the Src attribute of the Content element above. -->
    <Properties>
      <Property Name="SensitiveViewLists" Type="string" WebBrowsable="true" WebDisplayName="Lists" WebDescription="Lists to Query" WebCategory="SensitiveView Properties" DefaultValue="" RequiresDesignerPermission="true" />
    </Properties>

  </ClientWebPart>
</Elements>
